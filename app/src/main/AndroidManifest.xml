<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hehuidai.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--柱状图-->
        <activity android:name="com.hehuidai.customview.histogramview.HistogramActivity"/>
        <!--阴影布局-->
        <activity android:name="com.hehuidai.customview.shadownview.ShadownActivity"/>
        <!--文字动画-->
        <activity android:name="com.hehuidai.customview.animtextview.AnimTextViewActivity"/>
        <!--文字动画2-->
        <activity android:name=".animtextview2.AnimTextView2Activity"/>
    </application>

</manifest>